<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kuizei.po.TLog" table="t_log" >
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="uid">
            <column name="uid" sql-type="varchar(3)" length="3" />
        </property>
        <property name="optTime">
            <column name="opt_time" sql-type="timestamp" />
        </property>
        <property name="optLocation">
            <column name="opt_location" sql-type="varchar(3)" length="3" />
        </property>
        <property name="optId">
            <column name="opt_id" sql-type="varchar(10)" length="10" />
        </property>
        <property name="optType">
            <column name="opt_type" sql-type="varchar(2)" length="2" />
        </property>
    </class>
</hibernate-mapping>
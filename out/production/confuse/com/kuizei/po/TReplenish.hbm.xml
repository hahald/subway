<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.kuizei.po.TReplenish" table="t_replenish" >
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="cardId">
            <column name="card_id" sql-type="varchar(30)" length="30" />
        </property>
        <property name="money">
            <column name="money" sql-type="decimal(10)" precision="10" />
        </property>
        <property name="optTime">
            <column name="opt_time" sql-type="timestamp" />
        </property>
        <property name="optType">
            <column name="opt_type" sql-type="varchar(1)" length="1" />
        </property>
        <property name="optLocation">
            <column name="opt_location" sql-type="varchar(3)" length="3" />
        </property>
    </class>
</hibernate-mapping>